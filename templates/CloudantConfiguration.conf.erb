overrides = {

    fitnesskeeper.device = {
        CloudantDeviceStore = {
            cloudantURI = "https://<%= @cloudant_user -%>:<%= @cloudant_password -%>@<%= @cloudant_host -%>/rk-device<%= @cloudant_suffix -%>"
            documentType = "fitnesskeeper.core.entity.DeviceEntity"
        }
    }

    fitnesskeeper.notification = {
        CloudantScheduledPushNotificationStore = {
            cloudantURI = "https://<%= @cloudant_user -%>:<%= @cloudant_password -%>@<%= @cloudant_host -%>/rk-scheduled_push_notification2<%= @cloudant_suffix -%>"
            documentType = "fitnesskeeper.core.entity.PushNotificationEntity"
        }
    }

   fitnesskeeper.measurement = {
        FitnessKeeperWeightMeasurementService = {
            cloudantURI = "https://<%= @cloudant_user -%>:<%= @cloudant_password -%>@<%= @cloudant_host -%>/rk-weight_measurements<%= @cloudant_suffix -%>"
            documentType = "fitnesskeeper.core.entity.measurement.weight.AbstractWeightMeasurementEntity"
        }
    }

    runkeeper.apps = {
      ForeignAuthorizationManager = {
         cloudantURI = "https://<%= @cloudant_user -%>:<%= @cloudant_password -%>@<%= @cloudant_host -%>/rk-foreign_credentials<%= @cloudant_suffix -%>"
         documentType = "runkeeper.apps.ForeignCredentials"
      }
    }
}

<%= @conf_tier %> = ${overrides}
