{
    "file": {
        "/etc/security/limits.d/default_nofile.conf": {
            "exists": true,
            "mode": "0640",
            "owner": "root",
            "group": "root",
            "filetype": "file",
            "contains": []
        },
        "/etc/sysctl.d/10-rk_tomcat.conf": {
            "exists": true,
            "mode": "0644",
            "owner": "root",
            "group": "root",
            "filetype": "file",
            "contains": []
        },
        "/root/deploy.sh": {
            "exists": true,
            "mode": "0755",
            "owner": "root",
            "group": "root",
            "filetype": "file",
            "contains": []
        },
        "<%= @catalina_home %>/lib/<%= @postgres_driver %>.jar": {
            "exists": true,
            "mode": "0644",
            "owner": "root",
            "group": "root",
            "filetype": "file",
            "contains": []
        },
        "<%= @catalina_home %>/conf/catalina.properties": {
            "exists": true,
            "mode": "0664",
            "owner": "<%= @tomcat_user %>",
            "group": "<%= @tomcat_group %>",
            "filetype": "file",
            "contains": []
        },
        "<%= @catalina_home %>/newrelic": {
            "exists": true,
            "mode": "0750",
            "owner": "<%= @tomcat_user %>",
            "group": "<%= @tomcat_group %>",
            "filetype": "directory",
            "contains": []
        },
        "<%= @catalina_home %>/newrelic/logs": {
            "exists": true,
            "mode": "0750",
            "owner": "<%= @tomcat_user %>",
            "group": "<%= @tomcat_group %>",
            "filetype": "directory",
            "contains": []
        },
        "<%= @catalina_home %>/newrelic/newrelic.jar": {
            "exists": true,
            "mode": "0750",
            "owner": "<%= @tomcat_user %>",
            "group": "<%= @tomcat_group %>",
            "filetype": "file",
            "contains": []
        },
        "<%= @tomcat_logdir %>": {
            "exists": true,
            "mode": "0751",
            "owner": "<%= @tomcat_user %>",
            "group": "root",
            "filetype": "directory",
            "contains": []
        },
        "/usr/local/bin/saveCrashDump.rb": {
            "exists": true,
            "mode": "0755",
            "owner": "root",
            "group": "root",
            "filetype": "file",
            "contains": []
        }
    },
    "package": {
       "<%= @tomcat_native_pkg %>": {
            "installed": true,
            "versions": {
                "semver-constraint": ">=1.2.21 <1.3.0"
            }
        },
        "<%= @tomcat_pkg %>": {
            "installed": true,
            "versions": {
                "semver-constraint": ">=7.0.0 <7.1.0"
            }
        },
        <%- @font_pkgs.each do |font_pkg| -%>
        "<%= font_pkg %>": {
            "installed": true
        },
        <%- end -%>

    },
    "service": {
       "<%= @tomcat_svc %>": {
            "enabled": true,
            "running": false
        }
    },
    "user": {
       "<%= @tomcat_user %>": {
            "exists": true,
            "groups": [
               "<%= @tomcat_group %>"
            ]
        }
    },
    "group": {
       "<%= @tomcat_group %>": {
            "exists": true
        }
    },
    "dns": {
       "localhost": {
            "resolveable": true,
            "addrs": [
                "127.0.0.1"
            ]
        }
    }
}
